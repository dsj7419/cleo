{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "issues.create response",
  "description": "Response from creating a GitHub issue",
  "type": "object",
  "properties": {
    "_meta": {
      "type": "object",
      "properties": {
        "gateway": { "type": "string" },
        "domain": { "type": "string", "const": "issues" },
        "operation": { "type": "string" },
        "version": { "type": "string" },
        "timestamp": { "type": "string", "format": "date-time" },
        "duration_ms": { "type": "number" }
      },
      "required": ["gateway", "domain", "operation", "version", "timestamp", "duration_ms"]
    },
    "success": { "type": "boolean" },
    "data": {
      "type": "object",
      "properties": {
        "result": {
          "type": "object",
          "properties": {
            "type": { "type": "string", "enum": ["bug", "feature", "help"] },
            "url": { "type": "string", "format": "uri" },
            "number": { "type": ["number", "string"] },
            "title": { "type": "string" },
            "labels": {
              "type": "array",
              "items": { "type": "string" }
            }
          }
        }
      }
    },
    "error": {
      "type": "object",
      "properties": {
        "code": { "type": "string" },
        "exitCode": { "type": "number" },
        "message": { "type": "string" },
        "fix": { "type": "string" }
      }
    }
  },
  "required": ["_meta", "success"]
}
